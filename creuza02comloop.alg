algoritmo "Creuza02"
var
reais: real
dolares: real
c: inteiro
q: inteiro
inicio
c <-1
escreva("Quantas vezes você quer converter? ")
leia(q)
enquanto (c <= q) faca
   escreva("Quando dinheiro eu tenho? ")
   leia(reais)
   dolares <- reais/5.38
   escreval("Posso ter US$ " ,dolares )
   c <- c+1
fimenquanto
fimalgoritmo
